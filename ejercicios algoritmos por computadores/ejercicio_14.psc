Proceso ejercicio_14
	//Pedir el radio de una circunferencia y calcular su longitud.
	//DEFINIR VARIANBLES 
	Definir Radio, Long como Real;
	//ENTRADA DE DATOS 
	Escribir "Ingrese el valor de radio ";
	Leer Radio;
	//OPERACIONES 
	Long <- Radio * 2 * PI;
	//SALIDA
	Escribir "La longitud del circulo es: ", Long;

	
FinProceso
