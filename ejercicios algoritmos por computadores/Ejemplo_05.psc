Algoritmo Ejemplo_05
	//Ejemplo_05
	//Solicitar dos numeros y mostrar la suma de ambos 
	Definir Numero1, Numero2, Resultado como Real;
	Escribir "Difgite el primer numero";
	Leer Numero1;
	Escribir "Digite el segundo numero";
	Leer Numero2;
