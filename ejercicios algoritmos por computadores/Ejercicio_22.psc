Proceso Ejercicio_22
	//Pedir dos números y mostrarlos ordenados de mayor a menor.
	//DEFINIR VARIABLES 
	Definir valor1, valor2 como Real;
	//ENTRADA DE DATOS 
	Escribir "Ingrese dos valores para ordenarlos de mayor a menor";
	Leer valor1, valor2;
	//OPERACIONES 
	Si (valor1> valor2) Entonces
		Escribir "Orden de mayor a menor: ",valor1," - ", valor2;
	SiNo 
		Escribir "Orden de mayor a menor: ",valor2," - ", valor1;
	FinSi
	
FinProceso
