Proceso ejecicio_16 
	//Calcular el área de un rectángulo de lados X e Z
	//DECLARACION DE VARIBLES 
	Definir Area, X, Z como real;
	//Entrada
	Escribir "Ingrese el lado x ";
	Leer X;
	Escribir "Ingrese el lado Z ";
	Leer Z;
	//Operaciones 
	Area <- X*Z;
	//SALIDA 
	Escribir "El area del rectangulo es: ", Area;
	
FinProceso
