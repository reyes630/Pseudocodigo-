Proceso Ejercicio_27
	//Pedir un número y decir si es par o impar.
	//DEFINIR VARIABLES 
	Definir numerox como Entero;
	//ENTARADA DE DATOS 
	Escribir "Ingrese un numero para saber si es par o impar";
	Leer numerox;
	//PROCESO Y SALIDA 
	Si (numerox % 2 = 0) Entonces
		Escribir "El numero es par";
	SiNo
		Escribir "El numero es impar";
	FinSi

	
FinProceso
